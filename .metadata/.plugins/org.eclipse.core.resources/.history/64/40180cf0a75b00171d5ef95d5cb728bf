package com.wbl.JavaProblem;

import java.util.ArrayList;
import java.util.List;

public class StackImplementation {
	
	
	
	
	public static void main(String args[] ) {

		List<Integer> arr = new ArrayList<Integer>();
	//	arr = {2,4,5,7,8};
		//createStack(arr);
//		int num1 = 4;
		pushNum(arr, 2);
		pushNum(arr, 4);
		pushNum(arr, 5);
		pushNum(arr, 7);
		pushNum(arr, 8);

		int num2 = 10;
		popNum(arr);


		}

		public static void pushNum(List<Integer> numArr, int num1) {


		numArr.add(num1);

		}

		public static void popNum(List<Integer> numArr) {

		int temp = numArr.size();


		numArr.remove(temp-1);

        System.out.println(numArr);
		}

}
